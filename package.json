{
	"name": "close-call",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"prisma:inline": "cp ./node_modules/.prisma/client/*.js ./node_modules/@prisma/client",
		"prisma:migrate": "prisma migrate deploy && npm run prisma:inline",
		"prisma:migrate:dev": "prisma migrate dev && npm run prisma:inline",
		"prisma:generate": "prisma generate && npm run prisma:inline",
		"vercel-build": "npm run prisma:generate && prisma migrate deploy && npm run prisma:inline && vite build"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"prisma": "^5.16.2",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"ts-node": "^10.9.2",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.3"
	},
	"type": "module",
	"dependencies": {
		"@prisma/client": "^5.16.2",
		"arktype": "^2.0.0-dev.29",
		"svelte-maplibre": "^0.9.9"
	}
}
